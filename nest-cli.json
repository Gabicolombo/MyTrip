{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/MyTrip/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/MyTrip/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/MyTrip",
  "projects": {
    "MyTrip": {
      "type": "application",
      "root": "apps/MyTrip",
      "entryFile": "main",
      "sourceRoot": "apps/MyTrip/src",
      "compilerOptions": {
        "tsConfigPath": "apps/MyTrip/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "crypto": {
      "type": "library",
      "root": "libs/crypto",
      "entryFile": "index",
      "sourceRoot": "libs/crypto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/crypto/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    }
  }
}